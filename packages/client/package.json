{
  "name": "@wemorize/client",
  "version": "0.1.0",
  "description": "Wemorize web frontend",
  "main": "src/index.ts",
  "scripts": {
    "css-lint": "stylelint scss/",
    "css-prefix": "postcss --replace target/css/* --use autoprefixer --no-map",
    "css": "sass --style compressed --no-error-css --no-source-map --load-path=../node_modules scss:target/css && npm run css-prefix",
