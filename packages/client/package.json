  "license": "MIT",
  "dependencies": {
    "@wemorize/common": "^0.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "postcss-cli": "^11.0.0"
  }
}
